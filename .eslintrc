{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "plugin:prettier/recommended",
    "next"
  ],
  "plugins": ["prettier", "import"],
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "node": {
        "extensions": ["js", "jsx", "ts", "tsx"]
      },
      "typescript": true
    }
  }
}
